{
  "coc.preferences.formatOnSaveFiletypes": [
    "css",
    "html",
    "Markdown",
    "javascript",
    "typescript",
    "typescriptreact",
    "json",
    "javascriptreact",
    "typescript.tsx"
  ],
  "eslint.enable": true,
  "eslint.filetypes": [
    "javascript",
    "typescript",
    "typescriptreact",
    "javascriptreact",
    "typescript.tsx"
  ],
  "eslint.nodePath": ".yarn/sdks",
  "eslint.packageManager": "yarn",
  "workspace.workspaceFolderCheckCwd": false,
  "tsserver.enable": true,
  "tsserver.tsdk": ".yarn/sdks/typescript/lib"
}